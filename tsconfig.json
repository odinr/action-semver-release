{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "composite": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals":true,
    "noUnusedParameters":true,
    "esModuleInterop": true,
    "removeComments": true,
    "incremental": true
  },

}